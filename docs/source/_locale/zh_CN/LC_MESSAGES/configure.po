msgid "rcon.address"
msgstr "rcon.address"

#: ../configure.rst:147
msgid "The address of the rcon server"
msgstr "用于 rcon 连接的地址。"

#: ../configure.rst:151
msgid "Default value: ``127.0.0.1``"
msgstr "默认值：``127.0.0.1``"

#: ../configure.rst:154
